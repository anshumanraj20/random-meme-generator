{
  "manifest_version": 3,
  "name": "Random Meme Generator",
  "version": "1.0",
  "description": "Generate a random meme!",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "fav.png"
    }
  },
  "icons": {
    "128": "fav.png"
  },
  "options_ui": {
    "page": "options.html"
  }
}
